CLS
PRINT "Enter Date & Time"
PRINT " "
d$=DATE$
PRINT "Datum:"
FORM INPUT 10 AS d$
t$=TIME$
PRINT "Tid:"
FORM INPUT 10 AS t$
DATE$=d$
TIME$=t$
